{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:vue/vue3-essential"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["vue"],
	"rules": {
		"no-var": "error",
		"no-unused-vars": "warn",
		"no-dupe-keys": "off",
		"no-undef": "off"
	},
	"vue/return-in-computed-property": [
		"error",
		{
			"treatUndefinedAsUnspecified": false
		}
	]
}
